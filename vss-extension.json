{
  "manifestVersion": 1,
  "id": "vsts-task-publish-playstore",
  "name": "Publish to Google Play Store",
  "version": "1.0.1",
  "publisher": "chohfi",
  "public": true,
  "icons": {
    "default": "images/service-logo.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Publish your APK to Google Play Store (Alpha, Beta, Production or Rollout)",
  "categories": [
    "Build and release"
  ],
  "tags": [
    "google",
    "playstore",
    "apk",
    "publish",
    "store"
  ],
  "files": [
    {
      "path": "publishPlaystore"
    }
  ],
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE.md"
    }
  },
  "scopes": [
    "vso.build_execute"
  ],
  "links": {
    "getstarted": {
      "uri": "https://github.com/azchohfi/vsts-task-publish-playstore"
    },
    "support": {
      "uri": "https://github.com/azchohfi/vsts-task-publish-playstore/issues"
    }
  },
  "branding": {
    "color": "rgb(113, 107, 185)",
    "theme": "dark"
  },
  "contributions": [
    {
      "id": "publishPlaystore",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "publishPlaystore"
      }
    }
  ]
}